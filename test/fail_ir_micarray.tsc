<?xml version="1.0"?>
<!--
basic test: sound propagation, 2m
-->
<session license="CC-0">
  <scene>
    <source>
      <sound x="2" delayline="false" airabsorption="false"/>
      <orientation>0 0 0 0</orientation>
    </source>
    <receiver type="micarray">
      <mic delay="freefield" position="0 0 0">
        <mic name="Test Highshelf" delay="freefield" position="0 0 0">
          <filter type="highshelf" axis="0 1 0"/>
        </mic>
      </mic>
    </receiver>
  </scene>
</session>
